<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>Mathema.DiningByFriendsCosmosDB</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Gremlin.Net" />
    <None Remove="Newtonsoft.Json" />
    <None Remove="Gremlin.Net.Extensions" />
    <None Remove="Mathema.Bytecode4CosmosDB" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="Gremlin.Net" Version="3.4.13" />
    <PackageReference Include="Gremlin.Net.Extensions" Version="0.3.0" />
    <PackageReference Include="Mathema.Bytecode4CosmosDB" Version="0.1.0-alpha" />
  </ItemGroup>
</Project>
